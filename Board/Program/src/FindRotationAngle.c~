/**************************** Includes ****************************/

#include "FindRotationAngle.h"

/**************************** Function ****************************/

float FindRotationAngle () {

    AngleOfRotation = atan ( (QTY_OF_SENSORS * DISTANCE_BTW_SENSORS / 2) - (FindLog (2, SensorLineData) * DISTANCE_BTW_SENSORS / DISTANCE_WHEEL_TO_SENSORS) );

    return AngleOfRotation;

}
