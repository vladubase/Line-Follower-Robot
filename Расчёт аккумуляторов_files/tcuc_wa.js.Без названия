(function(){if("serviceWorker"in navigator){var d,e,b,f;window.addEventListener("load",function(){b=(e=document.getElementsByClassName("uc-install-app-block"))?e[0]:null;navigator.serviceWorker.register("/unit-converter/tcuc_sw.js").then(function(a){a="ServiceWorker registration successful with scope: "+a.scope;var c=document.getElementById("web-app-debug");c&&(c.innerHTML=a)},function(a){a="ServiceWorker registration failed: "+a;var c=document.getElementById("web-app-debug");c&&(c.innerHTML=a)});
b&&(f=b.getElementsByTagName("button")[0],window.addEventListener("beforeinstallprompt",function(a){b.style.display="block";d=a}),f.addEventListener("click",function(a){b.style.display="none";d.prompt();d.userChoice.then(function(a){d=null})}))})}})();
